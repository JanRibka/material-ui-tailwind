{
  "name": "@janribka/types",
  "version": "0.0.0",
  "private": false,
  "author": "Jan Ribka",
  "description": "Utility types for JanRibka",
  "license": "MIT",
  "type": "module",
  "types": "./index.ts",
  "files": [
    "index.d.ts",
    "OverridableComponentAugmentation.ts"
  ],
  "keywords": [
    "react",
    "react-components",
    "janribka",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/JanRibka/janribka",
    "directory": "packages/janribka-types"
  },
  "bugs": {
    "url": "https://github.com/JanRibka/janribka/issues"
  },
  "homepage": "https://github.com/JanRibka/janribka#readme",
  "scripts": {
    "dev": "tsup --watch --sourcemap",
    "build": "tsup && cpy OverridableComponentAugmentation.ts build/ && pnpm build:copy-files",
    "build:copy-files": "node ../../scripts/copyFiles.mjs",
    "prebuild": "rimraf build",
    "release": "pnpm build && pnpm version --no-git && pnpm publish --access public",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit",
    "typescript": "tsc -p tsconfig.json"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "devDependencies": {
    "@janribka/types": "workspace:*",
    "@types/react": "^18.3.3"
  },
  "peerDependencies": {
    "@types/react": "^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "dependencies": {
    "react": "^18.3.1",
    "typescript": "^5.5.3"
  }
}
