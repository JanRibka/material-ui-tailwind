{
  "name": "@janribka/components",
  "version": "0.0.8",
  "private": false,
  "author": "Jan Ribka",
  "description": "React components library for my projects",
  "license": "MIT",
  "type": "module",
  "main": "./src/index.ts",
  "keywords": [
    "react",
    "react-components",
    "library",
    "janribka",
    "radix-ui",
    "tailwind",
    "material design"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/JanRibka/janribka",
    "directory": "packages/janribka-components"
  },
  "bugs": {
    "url": "https://github.com/JanRibka/janribka/issues"
  },
  "homepage": "https://github.com/JanRibka/janribka#readme",
  "scripts": {
    "dev": "tsup --watch --sourcemap",
    "build:test": "node ../../scripts/build.mjs modern",
    "build:types-test": "node ../../scripts/buildTypes.mjs",
    "build": "tsup && pnpm build:copy-files",
    "build:copy-files": "node ../../scripts/copyFiles.mjs",
    "prebuild": "rimraf build",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit",
    "release": "pnpm version --no-git && pnpm publish --access public",
    "typescript": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@janribka/system": "workspace:*",
    "@janribka/types": "workspace:^",
    "@janribka/utils": "workspace:*",
    "react-transition-group": "^4.4.5"
  },
  "devDependencies": {
    "@janribka/internal-babel-macros": "workspace:^",
    "@babel/runtime": "^7.24.7",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-transition-group": "^4.4.10",
    "react": "19.0.0-rc-f6cce072-20240723",
    "react-dom": "19.0.0-rc-f6cce072-20240723",
    "rimraf": "^6.0.1",
    "tailwind-variants": "^0.2.1",
    "tailwindcss": "^3.4.6",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "@types/react": "^18.0.0 || ^19.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
